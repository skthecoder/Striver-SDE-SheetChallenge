#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   //   Write your code here
    int n0= 0 ,n1 = 0,n2 = 0 ;
    int i;
    for(i=0;i<n;i++)
    {
        if(arr[i] == 0)
            n0++ ;
        if(arr[i] == 1)
            n1++ ;
        if(arr[i] == 2)
            n2++ ;
    }
    i = 0 ;
    while(n0--)
    {
        arr[i] = 0 ;
        i++ ;
    }
    while(n1--)
    {
        arr[i] = 1 ;
        i++ ;
    }
    while(n2--)
    {
        arr[i] = 2 ;
        i++ ;
    }
    
     
}
